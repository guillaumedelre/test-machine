{% extends "@App/layout.html.twig" %}

{% block content %}
    {{ parent() }}
    <table class="table table-hover table-striped">
        <thead></thead>
        <tbody>
        {% for result in results %}
            <tr>
                <td>{{ result[0]|raw }}</td>
                <td>{{ result[1]|raw }}</td>
                <td>{{ result[2]|raw }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
{% endblock %}